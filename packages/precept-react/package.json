{
    "name": "precept-react",
    "version": "0.0.0",
    "description": "Precept components built with React",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "scripts": {
        "build": "run-s clean build:*",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir dist/es",
        "clean": "rimraf dist"
    },
    "keywords": [
        "react",
        "precept",
        "components"
    ],
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/precept"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "nanoid": "^4.0.0",
        "uncontrollable": "^7.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/preset-react": "^7.18.6",
        "babel-loader": "^8.3.0",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^4.1.0"
    }
}
