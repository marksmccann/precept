{
    "name": "precept",
    "version": "0.1.0-alpha.0",
    "description": "The universal UI component library",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "scripts": {
        "build": "run-s clean build:*",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir dist/es",
        "clean": "rimraf dist"
    },
    "keywords": [
        "components",
        "user interface",
        "ui"
    ],
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/precept/packages/precept"
    },
    "dependencies": {
        "@babel/runtime": "^7.20.7",
        "prop-types": "^15.8.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^4.1.0"
    }
}
