{
    "name": "precept-vue",
    "version": "0.1.0-alpha.1",
    "description": "Precept components built with Vue",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "scripts": {
        "build": "run-s clean build:*",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir dist/es",
        "clean": "rimraf dist"
    },
    "keywords": [
        "vue",
        "precept",
        "components"
    ],
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/precept/packages/precept-vue"
    },
    "peerDependencies": {
        "vue": "^3.2.45"
    },
    "dependencies": {
        "nanoid": "^4.0.0",
        "precept": "^0.1.0-alpha.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@vue/babel-plugin-jsx": "^1.1.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^4.1.0"
    }
}
